{
  "admin": {
    "port": 8360,
    "workers": 1,
    "stickyCluster": false,
    "startServerTimeout": 3000,
    "reloadSignal": "SIGUSR2",
    "processKillTimeout": 10000,
    "jsonpCallbackField": "callback",
    "jsonContentType": "application/json",
    "jsonpContentType": "application/javascript",
    "errnoField": "errno",
    "errmsgField": "errmsg",
    "defaultErrno": 1000,
    "validateDefaultErrno": 1001,
    "uploader": {
      "type": "file",
      "file": {},
      "qcdn": {},
      "qiniu": {
        "access_key": "",
        "secret_key": "",
        "bucket": "",
        "domain": ""
      }
    },
    "__esModule": true,
    "default": {},
    "cache": {
      "type": "file",
      "file": {
        "timeout": 1,
        "cachePath": "/animaris/runtime/cache",
        "pathDepth": 1,
        "gcInterval": 1
      }
    },
    "model": {
      "type": "sqlite",
      "sqlite": {
        "logConnect": true,
        "logSql": true,
        "path": "/animaris/runtime/sqlite",
        "database": "matrix",
        "encoding": "utf8",
        "connectionLimit": 1,
        "prefix": ""
      }
    },
    "session": {
      "type": "file",
      "file": {
        "cookie": {
          "name": "thinkjs"
        },
        "sessionPath": "/animaris/runtime/session"
      }
    },
    "view": {
      "type": "ejs",
      "ejs": {
        "viewPath": "/animaris/view",
        "extname": ".html",
        "sep": "_"
      }
    },
    "logger": {
      "type": "console",
      "console": {},
      "file": {
        "backups": 10,
        "absolute": true,
        "maxLogSize": 51200,
        "filename": "/animaris/logs/app.log"
      },
      "dateFile": {
        "level": "ALL",
        "absolute": true,
        "pattern": "-yyyy-MM-dd",
        "alwaysIncludePattern": true,
        "filename": "/animaris/logs/app.log"
      }
    }
  },
